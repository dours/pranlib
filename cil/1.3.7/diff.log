+ (** Performs alias analysis on the specified function and constructs module containing analysis results *)
+ module PranlibAliasAnalysisResults (FD : FuncReference) :
+   sig 
+     (** Descriptor of possibly infinite set of program states *)
+     type state 
+     
+     (* [mayAlias s e1 e2] returns true iff two expressions may alias in some program state 
+        descripted by [s] *)   
+     val mayAlias : state -> Cil.exp -> Cil.exp -> bool
+ 
+     (* [mustAlias s e1 e2] returns true iff two expressions must alias in any program state 
+        descripted by [s] *)  
+     val mustAlias : state -> Cil.exp -> Cil.exp -> bool
+ 
+     (* [assertsIsOneOf s] returns list of pairs (e, e_i) such that for every program state descripted by [d]
+        (e == &e_1 || ... e == &e_n) is true
+      *)
+     val assertsIsOneOf : state -> (Cil.lval * Cil.lval list) list 
+ 
+     (** [aliasInfoAfter s] returns descriptor of states in which program may be after execution of statement [s]
+         if such information is presented *)
+     val aliasInfoAfter : Cil.stmt -> state option
+ 
+     (** [aliasInfoAfter s] returns descriptor of states in which program may be before execution of statement [s]
+         if such information is presented *)
+     val aliasInfoBefore : Cil.stmt -> state option
