+   iterGlobals file (function
+     | GFun (f, _) -> (   
+       (*We rely on that these functions will be called from cilPranlibAA *)
+       (*prepareCFG f; computeCFGInfo f true;*) 
+ 
+       let module FuncRef = struct let file = file let func = f end in
+       let module AliasAnalysisResults = PranlibAliasAnalysisResults (FuncRef) in  
+       let mkAssertions position stmt =
+         let aliasInfo = match position with
+           `Before -> AliasAnalysisResults.aliasInfoBefore stmt
+         | `After  -> AliasAnalysisResults.aliasInfoAfter stmt
+         in
+         let mkOnFailedStmt = mkAssertFailedReportStmt file (Cil.get_stmtLoc stmt.skind) nextIdGen in
+         let assertionStmts state = List.fold_left
+           (fun acc a -> match a with 
+            | _, _, [] -> acc
+            | aType, var, vals -> let varExp = Cil.Lval var and
+                                      valsExps = List.map Cil.mkAddrOf vals
+                                    in
+                                    addToStat fileStat (List.length valsExps) aType;
+                                    (mkAssertion aType varExp valsExps (mkOnFailedStmt ())) :: acc
+           )
+           []
+           (AliasAnalysisResults.Asserts.asserts state)
+         in 
+         match aliasInfo with 
+           None -> []
+         | Some state -> assertionStmts state
+       in
+       let mkInstrumentedStmt stmt = match (mkAssertions `Before stmt, mkAssertions `After stmt) with
+           ([], [])        -> stmt 
+         | (before, after) -> mkStmtFromStmtList (List.concat [before; [stmt]; after])
+       in
+       let instrumentBlock block = block.bstmts <- List.map mkInstrumentedStmt block.bstmts
+       in        
+       (* iterate all function blocks and replace statements with instrumented ones *)
+       iterBlocksLeafsToRoots instrumentBlock f.sbody;
+       )     
+     | _ -> ()
+   );
+   logFileStat fileStat
+ 
+ 
+    
+ 
+ let feature : featureDescr = 
+   { fd_name = "pranlibAliasAsserts";              
+     fd_enabled = Cilutil.doPranlibAliasAsserts;
+     fd_description = "performs pranlib alias analysis and adds assert statements";
+     fd_extraopt = [("--assertStatFile", Arg.String (fun s -> statOut := open_out_gen [Open_append; Open_creat] 33188 s), 
+        " name of the file to which statistics should be printed")];
+     fd_doit = start;
+     fd_post_check = true
+   } 
+ 
