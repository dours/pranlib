+ open Cil
+ open CilPranlibAA
+ open AliasView
+ open Printf 
+ 
+ (* DEBUG FUNCTIONS *)
+ (*let out = (open_out_gen [Open_append] 0 "zzz")*)
+ (*END OF DEBUG FUNCTIONS *)
+ 
+ let optionMap f default = function
+   | Some v -> f v
+   | None   -> default
+ 
+ let noAttributes = []
+ 
+ let loggerFunctionName = "ReportAliasAnalysisAssertFailure"
+ let loggerFunctionType =
+   Cil.TFun (
+     Cil.voidType,
+     Some ( [("file", Cil.charPtrType, noAttributes);
+             ("line", Cil.intType, noAttributes);
+             ("assertId", Cil.intType, noAttributes)
+            ]
+          ),
+     false, noAttributes
+   )
+ 
+ let mkIdGenerator () = let id = ref 0 in (fun () -> let old = !id in incr id;  old)
+ 
+ let mkDummyLocation () = {line = -1; file = ""; byte = 0}
+ 
+ let mkBlockOneStmt stmt = Cil.mkBlock [stmt]
+ 
+ let mkEmptyBlock () = Cil.mkBlock []
+ 
+ let mkStubReturnStmt () = Cil.mkStmt (Cil.Return (None, mkDummyLocation ()))
+ 
+ let mkVarExp v = (Cil.Lval (Cil.var v))
+ 
+ let mkStmtFromStmtList stmts = Cil.mkStmt (Cil.Block (Cil.mkBlock stmts))
+ 
+ let mkEqualsToOneOfAssertion exp exprs onFailStmt =
+   let testType = Cil.TInt (Cil.IBool, []) in
+   let equalityTest e =  Cil.BinOp (Cil.Eq, exp, e, testType) in
+   let either e1 e2 = Cil.BinOp (Cil.LOr, e1, e2, testType) in
+   let equalities = List.map equalityTest exprs in
+   match equalities with 
+     [] -> failwith "Unexpected empty expression list in mkEqualsToOneOfAssertion."
+   | hd :: tl -> 
+     let testExp = List.fold_left either hd tl in
+     Cil.mkStmt (
+       Cil.If (testExp, mkEmptyBlock (), mkBlockOneStmt onFailStmt, mkDummyLocation ())
+     )
+ 
+ let mkAssertFailedReportStmt file location genAssertId () =
+   let loggerFunction = Cil.findOrCreateFunc file loggerFunctionName loggerFunctionType in
+   let funExp = mkVarExp loggerFunction and
+       argExps = [(Cil.mkString location.file); (Cil.integer location.line); (Cil.integer (genAssertId ()))] 
+   in
+   let printErrorCall = Cil.Call (None, funExp, argExps, mkDummyLocation ()) in
+   let errorStmt = Cil.mkStmtOneInstr printErrorCall in
+   errorStmt
+ 
+ let rec iterBlocksLeafsToRoots f root = 
+   let rec aux block = 
+     let iterStmt = function 
+     | Instr _ | Return _ | Goto _ -> ()
+     | If (_, b1, b2, _)           -> aux b1; aux b2
+     | Block b | Loop (b, _, _, _) -> aux b
+     | _                           -> failwith "iterBlocksLeafsToRoots: didn't wait stmt of that kind"
+     in List.iter (fun stmt -> iterStmt stmt.skind) block.bstmts;
+        f block
+    in aux root
+ 
+ (** TODO
+    1. commit
+    2. negative asserts
+    3. test on many programs
+  *)
+ 
+ 
+ let start file =
+   let nextIdGen = mkIdGenerator () in
+   iterGlobals file (function
+     | GFun (f, _) -> (
+       (*We rely on that these functions will be called from cilPranlibAA *)
+      (*prepareCFG f; computeCFGInfo f true;*) 
+ 
+       (*Printf.fprintf out "%s %s" file.fileName f.svar.vname;*)
+ 
+       let module FuncRef = struct let file = file let func = f end in
+       let module AliasAnalysisResults = PranlibAliasAnalysisResults (FuncRef) in  
+       let assertsForState = optionMap AliasAnalysisResults.assertsIsOneOf [] in
+       let mkAssertions position stmt =
+         let aliasInfo = match position with
+           `Before -> AliasAnalysisResults.aliasInfoBefore
+         | `After  -> AliasAnalysisResults.aliasInfoAfter
+         in
+         let mkOnFailedStmt = mkAssertFailedReportStmt file (Cil.get_stmtLoc stmt.skind) nextIdGen in
+         let assertionStmts = List.fold_left
+           (fun acc (var, vals) ->
+              if List.length vals = 0 
+              then acc
+              else let varExp = Cil.Lval var and valsExps = List.map Cil.mkAddrOf vals in
+                   (mkEqualsToOneOfAssertion varExp valsExps (mkOnFailedStmt ())) :: acc
+           )
+           []
+           (assertsForState (aliasInfo stmt))
+         in
+         match assertionStmts with 
+           [] -> None
+         | _  -> Some (Cil.mkStmt (Cil.Block (Cil.mkBlock assertionStmts)))
+       in
+       let mkInstrumentedStmt stmt = 
+         let withAfterAssert = optionMap (fun assrt -> [stmt; assrt]) [stmt] (mkAssertions `After stmt)
+         in
+         let withBeforeAssert = optionMap (fun assrt -> assrt :: withAfterAssert) withAfterAssert (mkAssertions `Before stmt)
+         in 
+         match withBeforeAssert with
+           [stmt] -> stmt
+         | lst    -> mkStmtFromStmtList lst
+       in
+       let updateRef = ref (fun stmtRef -> ())
+       in
+       let instrumentBlock block =
+         block.bstmts <- List.map (fun s ->
+           let is = mkInstrumentedStmt s and oldUpdate = !updateRef in 
+             updateRef := (fun sRef -> if !sRef == s then sRef := is else (oldUpdate sRef));
+             is
+         ) block.bstmts
+       in        
+       (* iterate all function blocks and replace statements with instrumented ones *)
+       iterBlocksLeafsToRoots instrumentBlock f.sbody;
+       (* update references to statements for all Goto operators *)
+       List.iter (function s -> match s.skind with Cil.Goto (sRef, _) -> !updateRef sRef | _ -> ()) f.sallstmts      
+       )      
+     | _ -> ()
+   )
+     
+ 
+ let feature : featureDescr = 
+   { fd_name = "pranlibAliasAsserts";              
+     fd_enabled = Cilutil.doPranlibAliasAsserts;
+     fd_description = "performs pranlib alias analysis and adds assert statements";
+     fd_extraopt = [];
+     fd_doit = start;
+     fd_post_check = true
+   } 
+ 
