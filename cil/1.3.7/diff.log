+ 	    let (graph, start), _ = O.removeEmptyNodes () in 
+ 	    let och = open_out (String.concat "" [f.svar.vname; ".dot"]) in
+ 	    Printf.fprintf och "%s" (Graph.DOT.toDOT graph);
+ 	    close_out och  
