+ let listZip l1 l2 = List.fold_right2 (fun e1 e2 acc -> (e1, e2) :: acc) l1 l2 []
+ 
+ let vSetToList set = VarSet.fold (fun v acc -> v :: acc) set []
+ 
+ let unOption = function | Some v -> v | None -> failwith "unOption"
+ 
+ let rec lastInList = function
+ | []       -> failwith "lastInList"
+ | [hd]     -> hd
+ | hd :: tl -> lastInList tl
+ 
+ let foldLeftWhile f p acc lst =
+   let rec aux acc = function
+   | []       -> acc
+   | hd :: tl -> if p hd
+                 then aux (f acc hd) tl
+                 else acc
+   in aux acc lst
+ (* ENDOF HELPER FUNCTIONS *)
+ 
+ let aliasGraphFile = ref ""
+ 
+ module BI = 
+   struct
+     type t = string * Region.t
+ 
+     let toString (n, r) = n   
+ 
+     let region = snd
+   end
+ 
+ module AnalysisFramework = Make (BI)  
+ 
+ module Memory = AnalysisFramework.M
+ module Block = Memory.Block
+ module Statement = AnalysisFramework.S
+ module Expression = Statement.Expr
+ 
+ 
+ (** Module providing a mapping from a region of memory visible (and accessible) from some function
+     to its "image" in a representation suitable for pranlib alias Results execution. *)
+ module CilToPranlibMemoryMap =
+   struct
+     (** Declarations of variables visible from some function. *)
+     module type Declarations =
+       sig
+         (** Global variables declarations. *)
+         val globals : Cil.varinfo list
+ 
+         (** Function formal arguments declarations. *)
+         val args : Cil.varinfo list
+ 
+         (** Local variables of a function.*)
+         val locals : Cil.varinfo list
+ 
+       end
+ 
+     
+     module type Sig =
+       sig
+         module D : Declarations
+   
+         (** returns expression representing block (or blocks) corresponding to variable denoted by varinfo*)
+         val getBlockExpr  : Cil.varinfo -> Expression.t option
+      
+         (** returns region allocated for given name *)
+         val getRegion : string  ->  Region.t
+     
+         (** [typeToBlockType r vname vtype] creates block type from a region [r], variable name [vname] and variable type [vtype]*)
+         val typeToBlockType : Region.t -> string -> typ -> Block.InfoTree.t option
+ 
+         (** [blockToLval b] returns C lvalue corresponding to this block (if one and only one such lvalue does exist) *)
+         val blockToLval : Block.t -> Cil.lval option
+       end
+ 
+   (** Constructs a memory mapping module. *)
+   module Make (D : Declarations) =
+     struct
+       module D = D
+ 
+       module BMap = Map.Make (Block)
+ 
+       let memoryRegionNamesList = ["Global"; "Local"; "Heap"]
+   
+       let varToExpr = Hashtbl.create 32     
+       let blockToLval = ref BMap.empty
+       let regs = ref []
+ 
+       let bindBlockToLval block lval =
+         blockToLval := (BMap.add block lval !blockToLval)
+       
+       let rec typeToBlockType region name = function
+        | TInt _ | TFloat _ | TPtr _ | TEnum _ | TArray _ | TBuiltin_va_list _ | TFun _ -> Some (Block.InfoTree.make (name, region) [])
+        | TComp (info, _)  when not(info.cstruct) -> Some (Block.InfoTree.make (name, region) [])
+        | TNamed (info, _) -> typeToBlockType region name info.ttype                    
+        | TComp (info, _) -> let fieldsInfo = Array.of_list info.cfields in
+                              Some ( Block.InfoTree.make
+                                       (name, region)
+                                       (Array.fold_right (fun el acc -> match typeToBlockType region el.fname el.ftype with    
+                                                                          Some t -> t :: acc
+                                                                        | None ->  raise (Failure "typeToBlockType")
+                                                         ) fieldsInfo []
+                                       )
+                                   )
+        | _ -> None
+ 
+       let rec isTypeContainingMergedBlocks = function
+       | TArray _ -> true
+       | TComp (info, _) when not(info.cstruct) -> true
+       | TComp (info, _) -> List.exists (fun fieldInfo -> isTypeContainingMergedBlocks fieldInfo.ftype) info.cfields
+       | TNamed (info, _) -> isTypeContainingMergedBlocks info.ttype
+       | _ -> false
+      
+       let regionName info = match info.vstorage with
+        | NoStorage -> if info.vglob then "Global" else "Local"
+        | Static    -> "Local"
+        | Register  -> if List.exists (fun elem -> (elem = info)) D.globals
+                       then "Global"
+                       else "Local"
+        | Extern    -> "Global"
+ 
+       let getBlockExpr info =
+         if Hashtbl.mem varToExpr info.vid
+         then Some (Hashtbl.find varToExpr info.vid)
+         else None
+      
+       let getRegion name =
+         try 
+           List.find (fun r -> (Region.name r) = name) !regs
+         with Not_found -> failwith ("Not found region " ^ name)
+            
+       let initRegions = fun () -> List.fold_left
+         (fun m name -> let (m, r) = Memory.createRegion ~name:name m in
+                          regs := r :: !regs;
+                          m
+         ) Memory.empty memoryRegionNamesList
+ 
+       let addVarToMemory varinfo memory = 
+         let bindLvalueMatchingBlock block =
+           let mkFieldOffsetLval finfo = Cil.addOffsetLval (Cil.Field (finfo, Cil.NoOffset)) in
+           let rec aux block vtype lval =
+             if not(isTypeContainingMergedBlocks vtype) 
+             then ( 
+               bindBlockToLval block lval;              
+               match vtype with
+                 TComp (cinfo, _) when cinfo.cstruct -> (
+                     List.iter (fun (finfo, sb) -> aux sb finfo.ftype (mkFieldOffsetLval finfo lval)) 
+                        (listZip cinfo.cfields  (Array.to_list (Block.subblocks block)))
+                   )
+               | _ -> ()
+             )
+           in
+           aux block varinfo.vtype (Cil.var varinfo)
+         in
+         let varRegion = getRegion (regionName varinfo)
+         in
+         match typeToBlockType varRegion varinfo.vname varinfo.vtype with
+           Some typ -> let (newMemory, b) = Memory.allocateBlock typ memory
+                       in
+                         bindLvalueMatchingBlock b;
+                         (* bind varinfo with correponding Alias expression *)
+                         if isTypeContainingMergedBlocks varinfo.vtype
+                         then  (
+                           let (newMemory', b') = Memory.allocateBlock typ newMemory in 
+                           Hashtbl.add varToExpr varinfo.vid (Expression.oneOf (Expression.block b) (Expression.block b'));
+                           newMemory'
+                         )
+                         else (
+                           Hashtbl.add varToExpr varinfo.vid (Expression.block b); 
+                           newMemory
+                         )                       
+          | None -> memory
+ 
+       let blockToLval block = 
+         if BMap.mem block !blockToLval
+         then Some (BMap.find block !blockToLval)
+         else None
+   
+       let init = fun () ->
+         let memory = initRegions () in
+         List.fold_left (fun memory varinfo -> addVarToMemory varinfo memory) memory (D.globals @ D.args @ D.locals)     
+                           
+     end
+   end
+ 
+ type position = Left | Right
+ 
+ module Abstractor (MemoryMap : CilToPranlibMemoryMap.Sig) =
+   struct
+  
+     module Concrete =
+       struct
+         type node = Graph.Node.t 
+         type edge = Graph.Edge.t
+       end
+   
+     module Abstract =
+       struct
+         type node = Statement.t list  
+         type edge = Graph.Edge.t
+       end
+ 
+     let getField i = function
+     | Some b -> let sbs = (Block.subblocks b) in
+                   if i < Array.length sbs
+                   then Some (sbs.(i))
+                   else None
+     | None -> None
+ 
+     let findFieldNumber fld = 
+         let rec find n = function
+           | (hd :: tl) -> if hd == fld then n else find (n + 1) tl
+           | [] -> raise (Failure "structure field not found")
+         in find 0 fld.fcomp.cfields
+ 
+     let rec lvalue =
+       let getField' i b = getField i (Some b)
+       in 
+       let rec makeOffset bl = function 
+       | NoOffset | Index _ ->  bl 
+       | Field (info, _) when not(info.fcomp.cstruct) -> bl 
+       | Field (info, off) -> makeOffset (Expression.sub (getField' (findFieldNumber info)) bl)  off 
+       in function
+       | Var (info), off -> (match MemoryMap.getBlockExpr info with
+                               Some bl -> makeOffset bl off   
+                             | None    -> Expression.undef
+                            )
+       | Mem (Lval l), off  -> makeOffset (Expression.value (lvalue l)) off
+       | Mem (AddrOf l), off -> makeOffset (lvalue l) off 
+       | Mem e, _ -> Expression.region (expr Left e)
+     and expr = fun x y -> match (x, y) with
+       | Left,  Lval l    -> lvalue l
+       | Right,  AddrOf l -> lvalue l
+       | Right, Lval l    -> Expression.value (lvalue l)
+       | Right, UnOp (_, e, _) | Right, BinOp (_, Const _, e, _) | Right, BinOp (_, e, Const _, _)
+                          -> expr Right e (* According to ANSI C semantics, pointer arithmetic is valid only within arrays
+                                             and we represent array as a monolite blocks *)
+       | x, CastE (_,  e) -> expr x e
+       | Right, Const _   -> Expression.undef
+       | _                -> Expression.any
+ 
+ 
+      let node x =   
+       let rec canSkipAssignmentTo = 
+         let rec aux = function 
+         | TInt _ | TFloat _ | TEnum _ -> true
+         | TNamed (info, _) -> aux info.ttype
+         | TArray (t, _, _) -> aux t
+         | _ -> false
+         in function   
+         | Var (info), _ -> aux info.vtype
+         | Mem (AddrOf l), _ -> canSkipAssignmentTo l
+         | _ -> false
+       in              
+       let exprrs = List.map (expr Right)
+       in
+       let node' = function
+       | Node.Call (Some lv, Lval (Var info, _), args) when
+          (match info.vtype with TFun _ -> true | _ -> false) &&
+          (match info.vname with "alloca" | "calloc" | "malloc" | "free" -> true | _ -> false) 
+                        -> 
+           (
+             let getAllocRegionName = function | "alloca" -> "Local" | _ -> "Heap"
+             in
+             let createAlloc t = [Statement.assign (lvalue lv) (Expression.alloc t)]
+             in
+             match info.vname with
+              | "malloc" | "alloca" | "calloc" ->
+                ( match MemoryMap.typeToBlockType
+                         (MemoryMap.getRegion (getAllocRegionName info.vname)) info.vname (Cil.typeOf (Cil.Lval lv))  with 
+                    Some t -> createAlloc t
+                  | None   -> raise (Failure "incorrect type of allocated block")
+                )
+              | "free" -> [Statement.assign (expr Left (List.hd args)) Expression.any]
+              | _      -> failwith "Unexpected operation with memory (cil2PranlibAA)"
+           )
+       | Node.Call (lv, f, args)->
+           let black = Statement.black [(MemoryMap.getRegion "Heap");] ((Expression.some (MemoryMap.getRegion "Global")) ::(exprrs args)) in
+             ( match lv with
+                 Some v -> (Statement.assign (lvalue v) Expression.any) :: [black] 
+               | _      -> [black]
+             )
+      | Node.Set (lv, e) ->
+         let makeStructAssignment lv e t = 
+           let cover = fun x -> Some x 
+             in
+             let rec aux subf = function
+               | TComp (info, _) -> List.concat (mapi (fun i fld -> aux (subf >> (getField i))  fld.ftype) info.cfields)
+               | _ -> [Statement.assign (Expression.sub (cover >> subf) lv) (Expression.sub (cover >> subf) e)] 
+             in aux (fun x -> x) t
+           in
+           (match Cil.typeOf (Cil.Lval lv) with
+             | TComp (info, _) as t -> makeStructAssignment (lvalue lv) (expr Right e) t
+             | _                    -> if not(canSkipAssignmentTo lv)
+                                       then [Statement.assign (lvalue lv) (expr Right e)]
+                                       else []
+           )
+       | Node.Start ->         
+           [Statement.black [(MemoryMap.getRegion "Heap");]
+                            (List.map (MemoryMap.getBlockExpr >> unOption) (MemoryMap.D.args @ MemoryMap.D.globals))]
+       | _ -> []
+       in node' (Graph.Node.info x)
+   
+   
+     let edge x = x
+ 
+   end
+ 
+ (* finds all global definitions visible from specified function *)
+ let findVisibleGlobalDecls file func =  
+   foldLeftWhile (fun acc -> function
+    | GVarDecl (info, _) | GVar (info, _, _) ->  VarSet.add info acc
+    | GFun (fdec, _) -> VarSet.add fdec.svar acc
+    | _ -> acc
+   )
+   (function | GFun (fdec, _) -> not (fdec == func) | _ -> true)
+   VarSet.empty
+   file.globals
+ 
+ module type FuncReference = 
+   sig    
+     val file : Cil.file
+     val func : Cil.fundec
+   end
+ 
+ module PranlibAliasAnalysisResults (FD : FuncReference) =
+   struct
+     (*let _ =
+       let s : string = FD.func.svar.vname in
+         Errormsg.log "%s\n" s*)
+ 
+     module PG = PranlibGraph.Make
+      (struct
+         let stmts =
+             prepareCFG FD.func; 
+             computeCFGInfo FD.func true;
+             FD.func.sallstmts
+       end)
+ 
+     module Declarations =
+       struct
+           let filter = 
+             let isInterestingVar vinfo = match Cil.unrollType vinfo.vtype with
+             | TComp _ | TPtr _ | TArray _ -> true
+             | _                           -> vinfo.vaddrof
+             in
+             List.filter isInterestingVar
+ 
+           let visibleGlobals = vSetToList (findVisibleGlobalDecls FD.file FD.func)
+ 
+           let globals = filter visibleGlobals
+           let args = filter FD.func.sformals
+           let locals = filter FD.func.slocals
+       end
+ 
+     module MemoryMap = CilToPranlibMemoryMap.Make (Declarations)
+     module A = Abstractor (MemoryMap)
+     module AnalysisResults = AnalysisFramework.Analyse (struct let memory = MemoryMap.init () end) (A) (PG)
+     module AliasAnalysisAsserts = AnalysisResults.Asserts
+     
+     type state = Before of PG.Node.t | After of PG.Node.t
+ 
+     let stateToAliasInfoGetter = function
+     | Before node -> AnalysisResults.before node
+     | After node  -> AnalysisResults.after node
+ 
+     let mayAlias st e1 e2 =
+       let f = stateToAliasInfoGetter st in
+       AnalysisResults.may (f (A.expr Left e1)) (f (A.expr Left e1))
+ 
+     let mustAlias st e1 e2 =
+       let f = stateToAliasInfoGetter st in
+       AnalysisResults.must (f (A.expr Left e1)) (f (A.expr Left e1))
+ 
+     module Asserts =
+       struct
+         let asserts st = 
+           let typeOfAssert = function
+           | AliasAnalysisAsserts.ValueIsOneOf _ -> `IsOneOf
+           | AliasAnalysisAsserts.ValueIsNotOneOf _ -> `IsNotOneOf
+           in
+           let mayBeUndefined block = 
+             let module E = AnalysisFramework.S.Expr in
+             let blockValueExpr = E.value (E.block block) in
+             (AnalysisResults.undefined ((stateToAliasInfoGetter st) blockValueExpr)) != None
+           in
+           let pointedTypeSig pType =
+             match Cil.unrollType pType with
+               TPtr (t, _) -> Cil.typeSig t
+             | _           -> failwith "pointedTypeSig"
+           in
+           let hasEqualTypeSig pointedTS lval = Cil.typeSig (Cil.typeOfLval lval) = pointedTS
+           in
+           let assertsForBlocks = match st with  
+             Before node -> AliasAnalysisAsserts.before node
+           | After node  -> AliasAnalysisAsserts.after node
+           in
+           List.fold_left (fun acc a -> match a with
+               AliasAnalysisAsserts.ValueIsOneOf (block, blockValues)
+             | AliasAnalysisAsserts.ValueIsNotOneOf (block, blockValues) -> 
+                 let aType = typeOfAssert a 
+                 in
+                 let pointerVarInfoOption = MemoryMap.blockToLval block
+                 in
+                 if pointerVarInfoOption = None || (aType = `IsNotOneOf && mayBeUndefined block)
+                 then acc
+                 else let pointer = unOption pointerVarInfoOption
+                      in           
+                      let pointerType = Cil.typeOfLval pointer
+                      in
+                      if not (Cil.isPointerType pointerType)
+                      then acc
+                      else let pointedLvalOptions = List.map MemoryMap.blockToLval blockValues
+                           in               
+                           if aType = `IsOneOf && List.mem None pointedLvalOptions
+                           then acc
+                           else let pointed = List.fold_left (fun acc opt ->  match opt with Some v -> v :: acc | None -> acc)
+                                                             [] pointedLvalOptions
+                                in
+                                let pointedTS = pointedTypeSig pointerType
+                                in
+                                let pointed'=  List.filter (hasEqualTypeSig pointedTS) pointed
+                                in
+                                (aType, pointer, pointed') :: acc
+            | _ -> acc
+           ) [] assertsForBlocks
+       end
+ 
+     let aliasInfoAfter stmt =
+       match stmt.skind with
+         If _ -> None (* does not make sense *)
+       | _    -> (match PG.findPranlibImage stmt with
+                    Some (`CFN node)  -> Some (After node)
+                  | Some (`Ins nodes) -> Some (After (lastInList nodes))
+                  | None              -> None
+                 ) 
+ 
+     let aliasInfoBefore stmt = 
+       match PG.findPranlibImage stmt with
+         Some (`CFN node)  -> Some (Before node)
+       | Some (`Ins nodes) -> Some (Before (List.hd nodes))
+       | None              -> None
+ 
+     let dot = lazy (AnalysisResults.DOT.toDOT())
+ 
+   end
+ 
+ 
+ let start file = 
+   iterGlobals file (function
+     | GFun (f, _) ->
+         let module FD = struct let file = file let func = f end
+         in
+         let module Result = PranlibAliasAnalysisResults (FD)
+         in 
+         if !aliasGraphFile <> ""
+         then ( let och = open_out !aliasGraphFile
+                in               
+                Printf.fprintf och "%s" (Lazy.force Result.dot);
+                close_out och
+              )
+     | _ -> ()
+   )
+     
+ 
+ let feature : featureDescr = 
+   { fd_name = "pranlibAA";              
+     fd_enabled = Cilutil.doPranlibAA;
+     fd_description = "building of the pranlib's CFG of the program and performing alias Results on it";
+     fd_extraopt = [
+       ("--aliasgraphfile", Arg.String (fun s -> aliasGraphFile := s), 
+        " name of the file where alias graph should be printed")
+     ];
+     fd_doit = start;
+     fd_post_check = true
+   } 
+ 
+ 
+ 
